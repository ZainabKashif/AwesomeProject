{
   "version": 2,
   "name": "awesome-project",
   "builds": [
      {
         "src": "*.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "handle": "filesystem"
      },
      {
         "src": "/api/(.*)",
         "dest": "/api/$1"
      },
      {
         "src": "/(.*)",
         "dest": "/"
      }
   ]
}
